{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "paths": { "*": ["./*"] },
    "outDir": "dist",
    "moduleResolution": "bundler",
    "resolveJsonModule": true
  }
}
